# Database Configuration
DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require

# Environment
NODE_ENV=development

# Messaging API Configuration
# Twilio SMS
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# WhatsApp Business API
WHATSAPP_ACCESS_TOKEN=your_whatsapp_access_token
WHATSAPP_PHONE_NUMBER_ID=your_whatsapp_phone_number_id
WHATSAPP_API_VERSION=v18.0

# Facebook Messenger
FACEBOOK_PAGE_ACCESS_TOKEN=your_facebook_page_access_token
FACEBOOK_VERIFY_TOKEN=your_facebook_verify_token

# Viber
VIBER_AUTH_TOKEN=your_viber_auth_token
VIBER_SENDER_NAME=your_viber_sender_name

# Vercel Blob Storage (for Payslip PDFs)
BLOB_READ_WRITE_TOKEN=vercel_blob_rw__FOLDERING_ERROR

